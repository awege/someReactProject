{
    "presets": ["env", "react"],
    "plugins": [
        ["transform-runtime", {
            "polyfill": false,
            "regenerator": true
          }],
        ["transform-react-jsx", {
          "pragma": "dom.bind({$$:$$})"
        }],
        ["transform-object-rest-spread", { "useBuiltIns": true }],
        ["transform-class-properties", { "useBuiltIns": true }]
      ]
}